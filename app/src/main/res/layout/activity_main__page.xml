<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main_Page">

    <ImageButton
        android:id="@+id/my_pets_btn"
        android:layout_width="85dp"
        android:layout_height="75dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="50dp"
        android:contentDescription="My Pets"
        android:padding="4dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/mypets" />

    <ImageButton
        android:id="@+id/pending_task_btn"
        android:layout_width="85dp"
        android:layout_height="75dp"
        android:layout_marginEnd="70dp"
        android:layout_marginTop="50dp"
        android:padding="4dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/clock" />

    <ImageButton
        android:id="@+id/pet_supplies_btn"
        android:layout_width="85dp"
        android:layout_height="75dp"
        android:layout_marginTop="50dp"
        android:padding="4dp"
        android:scaleType="fitCenter"
        app:layout_constraintTop_toBottomOf="@+id/cal_btn"
        app:srcCompat="@drawable/food"
        tools:layout_editor_absoluteX="70dp" />

    <ImageButton
        android:id="@+id/vet_care_btn"
        android:layout_width="85dp"
        android:layout_height="75dp"
        android:layout_marginTop="50dp"
        android:padding="4dp"
        android:scaleType="fitCenter"
        app:layout_constraintTop_toBottomOf="@+id/pending_task_btn"
        app:srcCompat="@drawable/health"
        tools:layout_editor_absoluteX="229dp" />

    <ImageButton
        android:id="@+id/cal_btn"
        android:layout_width="85dp"
        android:layout_height="75dp"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@+id/my_pets_btn"
        app:srcCompat="@android:drawable/ic_menu_my_calendar"
        tools:layout_editor_absoluteX="70dp" />

    <ImageButton
        android:id="@+id/help_btn"
        android:layout_width="85dp"
        android:layout_height="75dp"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@+id/vet_care_btn"
        app:srcCompat="@android:drawable/ic_menu_help"
        tools:layout_editor_absoluteX="229dp" />

</android.support.constraint.ConstraintLayout>